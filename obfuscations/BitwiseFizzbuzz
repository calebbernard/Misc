// bitwise fizzbuzz
// Caleb Bernard 2017
#include "iostream"
#include "string"

int main()
{
	unsigned char y = 'I', z = 'i';
	bool c = 0;
	unsigned int x = !c;
	for (int a = 0x16bc; a != 0x21bf; a = (a >> 1) | ((((a >> 0) ^ (a >> 2) ^ (a >> 3) ^ (a >> 5)) & 1) << 0xF)) {
		if (!c && z == 'i') {
			z = ((z << 1) | (z >> 7));
			c = !c;
		}
		else if (c && z == 'i')
			c = !c;
		if (c)
			z = ((z << 1) | (z >> 7));
		if (z & 1)
			std::cout << x << '\n';
		else {
			if (y == 'R') {
				std::cout << "FizzBuzz\n";
				y = ((y << 2) | (y >> 6));
			}
			else {
				y = ((y << 1) | (y >> 7));
				if (y & 1)
					std::cout << "Buzz\n";
				else
					std::cout << "Fizz\n";
			}
		}
		if (!c)
			z = ((z >> 1) | (z << 7));
		x ^= x&~- ~x | ~x&-~x;
	}
  return 0;
}
